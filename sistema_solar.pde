float ppy=t*cos(radians(venus)); 
point(ppx+x,ppy+y);
venus+=5;
//orvita luna
t=55;
x=400;
y=400;
a=17;
float pppxx=t*sin(radians(orvitaluna)); 
float pppyy=t*cos(radians(orvitaluna)); 
fill(0);
stroke(225,225,0);
strokeWeight(1);
ellipse(pppxx+x,pppyy+y,a,a);
